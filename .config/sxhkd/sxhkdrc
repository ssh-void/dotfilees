super+ KP_Enter
              st &> /dev/null 

super + x
        exec $HOME/.config/.script/img_txt_net.sh &> /dev/null  

super + c
        exec $HOME/.config/.script/img_txt.sh  &> /dev/null 

super + s
        exec $HOME/.config/.script/scratchpad.sh &> /dev/null 

alt + {Up,Down}
              exec $HOME/.config/.script/brightnessctl.sh {up,down}

XF86Audio{RaiseVolume,LowerVolume,Mute}
                                      exec $HOME/.config/.script/pamixer.sh {up,down,toggle}

super + p
        flameshot gui &> /dev/null

super + v
      	virt-manager &> /dev/null

super + {f,t,b}
              {firefox &> /dev/null ,torbrowser-launcher &> /dev/null ,brave-browser-stable &> /dev/null }

alt + b
      blueman-manager &> /dev/null 

alt + p 
      pavucontrol &> /dev/null 

super + l 
        xset dpms force suspend && slock  &> /dev/null 

super + e
        thunar &>/dev/null 

super + shift  + p
        $HOME/.config/.script/screenshot_region.sh &> /dev/null 
