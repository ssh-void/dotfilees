super + s
        exec $HOME/.config/.script/scratchpad.sh &> /dev/null

super+ KP_Enter
              exec $HOME/.config/.script/translate.sh

super + {x,c}
        {exec $HOME/.config/.script/img_txt_net.sh &> /dev/null,exec $HOME/.config/.script/img_txt.sh &> /dev/null}

alt + {Up,Down}
              exec $HOME/.config/.script/brightnessctl.sh {up,down}

XF86Audio{RaiseVolume,LowerVolume,Mute}
                                      exec $HOME/.config/.script/pamixer.sh {up,down,toggle}

super + alt + p
              maim -m 10 -s -c 0,0,1,1 -n 2 -b 2 -f png | xclip -selection clipboard -target image/png &> /dev/null

alt + p
              flameshot gui &> /dev/null

#super + v
#        virt-manager &> /dev/null

