super + x
        exec $HOME/.config/.script/img_txt_net.sh

super + c
        exec $HOME/.config/.script/img_txt.sh  

super + d
        dmenu_run -fn "Ubunto Font:style=Medium:size=15" -nb '#333333' -nf '#ffffff' -sb '#c61169' -sf '#f9f102' 

super + s
        exec $HOME/.config/.script/scratchpad.sh

alt + {Up,Down}
              exec $HOME/.config/.script/brightnessctl.sh {up,down}

XF86Audio{RaiseVolume,LowerVolume,Mute}
                                     exec $HOME/.config/.script/pamixer.sh {up,down,toggle}

super + p
        flameshot gui &> /dev/null

super + v
	 virt-manager &> /dev/null

super + shift + l 
                slock

super + {f,t,b}
              {firefox &> /dev/null,torbrowser-launcher &> /dev/null ,brave-browser-stable &> /dev/null}

alt + b
	      blueman-manager &> /dev/null

alt + p 
	pavucontrol &> /dev/null

alt + m 
      thunderbird &>/dev/null
